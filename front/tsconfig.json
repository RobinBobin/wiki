{
  "compilerOptions": {
    "paths": {
      "@commonComponents": ["./src/commonComponents"],
      "@enums": ["./src/enums"],
      "@gen/*": ["./src/gen/*"],
      "@helpers/*": ["./src/helpers/*"],
      "@hooks": ["./src/hooks"],
      "@mst": ["./src/mst"],
      "@mst/*": ["./src/mst/*"],
      "@routes": ["./src/routes"],
      "@types": ["./src/types"]
    }
  },
  "extends": ["expo/tsconfig.base", "@robinbobin/ts-config"],
  "files": ["eslint.config.js", "src/constants/appConfig.js"],
  "include": ["./**/*.ts", "./**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
